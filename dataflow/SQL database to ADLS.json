{
	"name": "SQL database to ADLS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RandomDWTable",
						"type": "DatasetReference"
					},
					"name": "dimCity"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "ADLSwwi",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          CityKey as integer,",
				"          WWICityID as integer,",
				"          City as string,",
				"          StateProvince as string,",
				"          Country as string,",
				"          Continent as string,",
				"          SalesTerritory as string,",
				"          Region as string,",
				"          Subregion as string,",
				"          Location as binary,",
				"          LatestRecordedPopulation as long,",
				"          ValidFrom as timestamp,",
				"          ValidTo as timestamp,",
				"          LineageKey as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimCity",
				"dimCity sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'wwidw',",
				"     folderPath: 'adfDeltaOutput',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          CityKey,",
				"          WWICityID,",
				"          City,",
				"          StateProvince,",
				"          Country,",
				"          Continent,",
				"          SalesTerritory,",
				"          Region,",
				"          Subregion,",
				"          Location,",
				"          LatestRecordedPopulation,",
				"          ValidFrom,",
				"          ValidTo,",
				"          LineageKey",
				"     )) ~> sink1"
			]
		}
	}
}